{
  "name": "new-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:acceptance": "cucumber-js",
    "test:all": "pnpm test && pnpm test:acceptance",
    "test:e2e": "playwright test",
    "test:e2e:visual": "playwright test visual.spec.js",
    "test:e2e:visual:update": "playwright test visual.spec.js --update-snapshots",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "svelte": "^5.16.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.6.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.7",
    "autoprefixer": "^10.4.23",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.3.3",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
